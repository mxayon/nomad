
<div class="card large center">
  <img class="center" src="<%= @post.picture %>">
  <div class="card-content">
    <span class="card-title activator grey-text text-darken-4">  <%= @post.title %> </span>
    <p><a href="#">This is a link @post.title</a></p>
    <%= @post.city.name %> <br />
    <br />
    <%= @post.content %>
  </div>
</div>
